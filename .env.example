# Environment variables for rag-study-assistant
# Copy to .env and adjust as needed
APP_ENV=development
LOG_LEVEL=INFO
DATA_ROOT=./data
DB_PATH=./data/db/rag_assistant.db
QDRANT_HOST=localhost
QDRANT_PORT=6333
QDRANT_COLLECTION=rag_study
QDRANT_URL=http://localhost:6333
QDRANT_VECTOR_SIZE=384
# Embeddings (local by default)
EMBEDDINGS_PROVIDER=local
EMBEDDINGS_MODEL=intfloat/multilingual-e5-small
EMBEDDINGS_VECTOR_SIZE=384
# Optional OpenAI-compatible settings (only needed if using openai provider)
# OPENAI_API_KEY=changeme
# OPENAI_BASE_URL=
